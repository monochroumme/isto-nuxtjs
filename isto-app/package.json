{
  "name": "isto",
  "version": "1.0.0",
  "description": "ISTO",
  "author": "rare",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "cross-env DEPLOY_ENV=GH_PAGES nuxt generate",
