  },
  "dependencies": {
    "@nuxtjs/axios": "^5.8.0",
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0-rc.11",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "nuxt": "^2.0.0",
    "nuxt-env": "^0.1.0",
    "nuxt-i18n": "^6.3.0"
  },
  "devDependencies": {
    "node-sass": "^7.0.0",
    "nodemon": "^1.18.9",
    "push-dir": "^0.4.1",
    "sass-loader": "^8.0.0",
    "vue-awesome-swiper": "^3.1.3"
  }
}
